.box {
  padding: 40rpx;
  box-sizing: border-box;
}

.noFinish_title,
.isFinish_title {
  display: flex;
  align-items: center;
  margin: 20rpx 0;

}

.title_w {
  font-size: 32rpx;
  font-weight: 700;
}

.title_num {
  font-size: 32rpx;
  color: #A8A8A8;
  margin-left: 10rpx;
}

.noFinish_content,
.isFinish_content {
  border-radius: 30rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, .1);
}

.detail {
  display: flex;
  align-items: center;
  padding: 30rpx 0;
  box-sizing: border-box;
  border-bottom: 1px solid #F2F2F2;
}

.detail:last-child {
  border: none;
}

.detail image {
  display: inline-block;
  width: 72rpx;
  height: 72rpx;
}

.detail_word {
  font-size: 26rpx;
  color: #A8A8A8;
  padding: 6rpx 20rpx;
  border-radius: 40rpx;
  border: 1px solid #A8A8A8;
}

.detail_title {
  display: block;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-weight: 700;
  font-size: 30rpx;
}

.detail_time {
  display: block;
  font-size: 26rpx;
  color: #A8A8A8;
}

.detail_word_box {
  width: 380rpx;
  margin-left: 30rpx;
}