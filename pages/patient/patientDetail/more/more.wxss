@import '../maintenance//maintenance.wxss';

.weui-search-bar {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
}

.weui-search-bar__form {
  border-radius: 40rpx !important;
}

.weui-icon-search_in-box {
  position: absolute;
  left: 40rpx;
  top: 5rpx;
}

.weui-search-bar__input {
  padding-left: 40rpx;
}

.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 5px;
  padding: 14rpx 0;
  background: #F5F5F5;
  border: 1px solid #E6E6EA;
}

.weui-search-bar__box {
  position: relative;
  padding-left: 60rpx;
  padding-right: 60rpx;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.weui-search-bar__input {
  height: 56rpx;
  line-height: 56rpx;
}
.maintainRecord {
  padding-left: 10rpx;
  margin: 30rpx 0;
}
.content {
  margin-bottom: 20rpx;
}