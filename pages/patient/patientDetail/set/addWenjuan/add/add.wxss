.weui-search-bar {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
}

.weui-search-bar__form {
  border-radius: 40rpx !important;
}

.weui-icon-search_in-box {
  position: absolute;
  left: 40rpx;
  top: 5rpx;
}

.weui-search-bar__input {
  padding-left: 40rpx;
}

.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 5px;
  padding: 14rpx 0;
  background: #F5F5F5;
  border: 1px solid #E6E6EA;
}

.weui-search-bar__box {
  position: relative;
  padding-left: 60rpx;
  padding-right: 60rpx;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.weui-search-bar__input {
  height: 56rpx;
  line-height: 56rpx;
}

.search {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  border-radius: 100rpx;
  background-color: #F5F5F5;
  color: #D3D3D3;
  font-size: 28rpx;
}

.iconfangdajing1 {
  margin-right: 5rpx;
}

.wenjuan_box {
  padding: 0 30rpx;
  border-radius: 30rpx;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, .1);
}

.wenjuanList {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 20rpx;
  border-bottom: 1px solid #f2f2f2;
  font-size: 30rpx;
  font-weight: 700;
  background-color: #fff;
  padding: 20rpx 0;
}

.wenjuanList:last-child {
  border: none;
}

.weui-search-bar__box {
  padding-right: 0;
}

.weui-search-bar__form {
  padding: 5rpx 0;
}

.btnSave {
  position: fixed;
  bottom: 80rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 70%;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 20rpx;
  box-shadow: 0 2px 12px 0 rgba(0, 0, 0, .1);
  color: #fff;
  box-sizing: border-box;
  border-radius: 120rpx;
  background-color: #1ADCA9;
}

.time {
  padding: 20rpx 30rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
  font-weight: 700;
}

.questionImg {
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.quesRecord {
  font-weight: 700;
}